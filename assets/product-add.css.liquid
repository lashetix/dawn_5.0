/*Flex Reverse Cow Girl*/
        .flex-container-left-on-top {
            display: flex;
            flex-wrap: wrap;
            text-align: left;
            margin: 0;
            align-items: center;
            justify-content: center;
        }
        
        .flex-container-right-on-top{
            display: flex;
            flex-wrap: wrap;
            text-align: left;
            align-items: center;
            justify-content: center;
        }
        
        .flex-item-left {
            padding: 5px;
            flex: 50%;
            width: auto;
            padding-left: 28px;
            padding-right: 28px;
            
        }
        
        .flex-img-left {
            padding: 0px;
            flex: 50%;
            width: auto;
            text-align:center;
            
        }
        
        .flex-item-right {
            padding: 5px;
            flex: 50%;
            width: auto;
            padding-left: 28px;
            padding-right: 28px;
        
        }
        
        .flex-img-right {
            padding: 0px;
            flex: 50%;
            width: auto;
            text-align:center;
        }
/*Flex Cow Girl*/
.flex-container {
    display: flex;
    flex-direction: row;
    font-size: 30px;
    text-align: center;
  }
/*Flex Two Row*/
  .flex-item-one-two {
    padding: 10px;
    flex: 50%;
  }
  
  .flex-item-two-two {
    padding: 10px;
    flex: 50%;
  }
  /*Flex Three Row*/
  .flex-item-one-three {
    padding: 10px;
    flex: 33.3333%;
  }
  
  .flex-item-two-three {
    padding: 10px;
    flex: 33.3333%;
  }
  
  .flex-item-three-three {
    padding: 10px;
    flex: 33.333%;
  }
/*Flex Four Row*/
.flex-item-one-four {
    padding: 10px;
    flex: 25%;
  }
  
  .flex-item-two-four {
    padding: 10px;
    flex: 25%;
  }
  
  .flex-item-three-four {
    padding: 10px;
    flex: 25%;
  }
  
  .flex-item-four-four {
    padding: 10px;
    flex: 25%;
  }

/* Less than 750px */
  @media (max-width: 749px) {

    /* Reverse CowGirl */
    .flex-container-left-on-top {
      flex-direction: column-reverse;}
    
      .flex-container-right-on-top{
      flex-direction: column;
    }
    .spacer{
      height: 5.5rem;
    }
    /* CowGirl */
    .flex-container {
        flex-direction: column;
    }
  }
/* Table Styles */
